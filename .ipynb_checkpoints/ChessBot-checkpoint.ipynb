    "    #Put in Pieces\n",
    "    board[0][0] = \"R\"\n",
    "    board[0][1] = \"K\"\n",
    "    board[0][2] = \"B\"\n",
    "    board[2][0] = \"r\"\n",
    "    board[2][1] = \"k\"\n",
    "    board[2][2] = \"b\"\n",
    "    \n",
    "    return board\n",
