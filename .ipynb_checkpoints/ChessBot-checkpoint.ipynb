    "#This method is used for deciding how the bot will make its moves\n",
    "def chooseMove(board):\n",
    "    bestRecord = float('-inf')\n",
    "    indexOfBM = 0\n",
    "    if(board in actualWLRecords):\n",
    "        for move in actualWLRecords[board]:\n",
    "            diff = move[1][0]/(move[1][1]+1)\n",
    "            if diff > bestRecord:\n",
    "                bestRecord = diff\n",
    "                indexOfBM = actualWLRecords[board].index(move)\n",
    "    else:\n",
    "        return randomMove(board)\n",
    "    return actualWLRecords[board][indexOfBM]\n",
    "        "
