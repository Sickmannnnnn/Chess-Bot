    "        \n",
    "\n",
    "#Tested\n",
    "def isValidPawnMove(board, piece, moveFrom, moveTo):\n",
    "    if(isNormalPawnMove(board, piece, moveFrom, moveTo) or isPawnTake(board, piece, moveFrom, moveTo)):\n",
    "        if(notOwnTeam(board, piece, moveTo) and isOnBoard(moveTo) and pieceExists(board, piece, moveFrom)):\n",
    "            return True\n",
