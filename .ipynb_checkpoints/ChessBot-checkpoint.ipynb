    "# This code block is for the bot training controls\n",
    "\n",
    "def findValidMovesUpper(board):\n",
    "    validMoves = [];\n",
    "    for row1 in range(len(board)):\n",
    "        for col1 in range(len(board[row1])):\n",
    "            if board[row1][col1].isupper():\n",
    "                for row2 in range(len(board)):\n",
    "                    for col2 in range(len(board[row2])):\n",
    "                        if(isValidMove(board, board[row1][col1], [row1,col1], [row2,col2])):\n",
    "                            validMoves.append([[row1,col1],[row2,col2]])\n",
    "    return validMoves\n",
    "\n",
    "def findValidMovesLower(board):\n",
    "    validMoves = [];\n",
    "    for row1 in range(len(board)):\n",
    "        for col1 in range(len(board[row1])):\n",
    "            if board[row1][col1].islower():\n",
    "                for row2 in range(len(board)):\n",
    "                    for col2 in range(len(board[row2])):\n",
    "                        if(isValidMove(board, board[row1][col1], [row1,col1], [row2,col2])):\n",
    "                            validMoves.append([[row1,col1],[row2,col2]])\n",
    "    return validMoves\n",
    "\n",
    "def playGame(board):\n",
    "    turnCount = 0\n",
    "    boardAndMoveArray = []\n",
    "    \n",
    "    #determines who plays first\n",
    "    firstMove = math.floor(random.random()*2)\n",
    "    \n",
    "    while(not gameOver(board)):\n",
    "        if(turnCount % 2 == firstMove):\n",
    "            tempBoard1 = copy.deepcopy(board)\n",
    "            move1 = findValidMovesLower(board)[random.randint(0,len(findValidMovesLower(board))-1)]\n",
    "            moveFrom1 = move1[0]\n",
    "            moveTo1 = move1[1]\n",
    "            piece1 = board[move1[0][0]][move1[0][1]]\n",
    "            movePiece(board, piece1, moveFrom1, moveTo1)\n",
    "            boardAndMoveArray.append([tempBoard1, [moveFrom1, moveTo1], \"Lower\"])\n",
    "        \n",
    "        else:\n",
    "            tempBoard2 = copy.deepcopy(board)\n",
    "            move2 = findValidMovesUpper(board)[random.randint(0,len(findValidMovesUpper(board))-1)]\n",
    "            moveFrom2 = move2[0]\n",
    "            moveTo2 = move2[1]\n",
    "            piece2 = board[move2[0][0]][move2[0][1]]\n",
    "            movePiece(board, piece2, moveFrom2, moveTo2)\n",
    "            boardAndMoveArray.append([tempBoard2, [moveFrom2, moveTo2], \"Upper\"])\n",
    "            \n",
    "        turnCount += 1\n",
    "            \n",
    "    return([boardAndMoveArray, whoWon(board)])\n",
    "\n",
    "\n",
    "\n",
    "def randomMove(board):\n",
    "    tempBoard1 = copy.deepcopy(board)\n",
    "    move1 = findValidMovesUpper(board)[random.randint(0,len(findValidMovesUpper(board))-1)]\n",
    "    moveFrom1 = move1[0]\n",
    "    moveTo1 = move1[1]\n",
    "    piece1 = board[move1[0][0]][move1[0][1]]\n",
    "    return [[moveFrom1, moveTo1], [0,0]]\n",
    "                           "
