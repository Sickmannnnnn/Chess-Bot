    "        return True\n",
    "    else: \n",
    "        return False\n",
    "\n",
    "    \n",
    "#Tested\n",
    "def notOwnTeam(board, piece, moveTo):\n",
    "    if piece.islower() and board[moveTo[0]][moveTo[1]].islower() or piece.isupper() and board[moveTo[0]][moveTo[1]].isupper():\n",
    "        return False\n",
    "    else:\n",
    "        return True\n",
    "    \n",
    "    \n",
    "#Tested\n",
    "def isOnBoard(moveTo):\n",
    "    if moveTo[0] < 0 or moveTo[0] > 2 or moveTo[1] < 0 or moveTo[1] > 2:\n",
    "        return False\n",
    "    else:\n",
    "        return True\n",
    "\n",
    "    \n",
    "#Tested\n",
    "def pieceExists(board, piece, moveFrom):\n",
    "    if(board[moveFrom[0]][moveFrom[1]] == piece and piece != \"-\"):\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "    \n",
    "#Tested\n",
    "def isValidRookMove(board, piece, moveFrom, moveTo):\n",
    "    if(inSameColumn(moveFrom, moveTo) or inSameRow(moveFrom, moveTo)):\n",
    "        if(doesntJumpR(board, moveFrom, moveTo)):\n",
    "            if(notOwnTeam(board, piece, moveTo) and isOnBoard(moveTo) and pieceExists(board, piece, moveFrom)):\n",
    "                return True\n",
    "    return False\n",
    "\n",
    "\n",
    "#Tested\n",
    "def isValidBishopMove(board, piece, moveFrom, moveTo):\n",
    "    if(isDiagonal(moveFrom, moveTo)):\n",
    "        if(doesntJumpB(board, moveFrom, moveTo)):\n",
    "            if(notOwnTeam(board, piece, moveTo) and isOnBoard(moveTo) and pieceExists(board, piece, moveFrom)):\n",
    "                return True\n",
    "    return False\n",
    "\n",
    "#Tested\n",
    "def isValidKingMove(board, piece, moveFrom, moveTo):\n",
    "    if(isKingMove(moveFrom, moveTo)):\n",
    "        if(notOwnTeam(board, piece, moveTo) and isOnBoard(moveTo) and pieceExists(board, piece, moveFrom)):\n",
    "            return True\n",
    "    return False\n",
    "   \n",
    "#Pawn Moves for expansion\n",
    "\"\"\"   \n",
