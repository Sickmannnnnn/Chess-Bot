    "    \n",
    "    \n",
    "#Tested\n",
    "def isPawnTake(board, piece, moveFrom, moveTo):\n",
    "    if(isOnBoard(moveTo)):\n",
    "        if(piece.islower()):\n",
    "            if(board[moveTo[0]][moveTo[1]].isupper()):\n",
    "                if(moveFrom[0]-1==moveTo[0] and (moveFrom[1]-1==moveTo[1] or moveFrom[1]+1==moveTo[1])):\n",
