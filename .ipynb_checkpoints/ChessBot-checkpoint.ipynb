    "        board[moveFrom[0]][moveFrom[1]] = \"-\"\n",
    "        board[moveTo[0]][moveTo[1]] = piece\n",
    "    else:\n",
    "        return False\n",
    "        \n",
    "        \n",
    "def isValidMove(board, piece, moveFrom, moveTo):\n",
    "    if (piece == \"r\" or piece == \"R\") and isValidRookMove(board, piece, moveFrom, moveTo):\n",
    "        return True\n",
    "    elif (piece == \"b\" or piece == \"B\") and isValidBishopMove(board, piece, moveFrom, moveTo):\n",
    "        return True\n",
    "    elif (piece == \"p\" or piece == \"P\") and isValidPawnMove(board, piece, moveFrom, moveTo):\n",
    "        return True\n",
    "    elif (piece == \"k\" or piece == \"K\") and isValidKingMove(board, piece, moveFrom, moveTo):\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "        \n",
    "#Tested\n",
    "def inSameRow(moveFrom, moveTo):\n",
    "    if(moveFrom[0] == moveTo[0]):\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "    \n",
    "    \n",
    "#Tested\n",
    "def inSameColumn(moveFrom, moveTo):\n",
    "    if(moveFrom[1] == moveTo[1]):\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "    \n",
    "#Tested\n",
    "def doesntJumpR(board, moveFrom, moveTo):\n",
    "    if(inSameRow(moveFrom, moveTo) and moveFrom[1] > moveTo[1]):\n",
    "        i = moveFrom[1]\n",
    "        while i > moveTo[1]+1:\n",
    "            i -= 1\n",
    "            if(board[moveTo[0]][i] != \"-\"):\n",
    "                return False\n",
    "    \n",
    "    elif(inSameRow(moveFrom, moveTo) and moveFrom[1] < moveTo[1]):\n",
    "        i = moveFrom[1]\n",
    "        while i < moveTo[1]-1:\n",
    "            i += 1\n",
    "            if(board[moveTo[0]][i] != \"-\"):\n",
    "                return False\n",
    "            \n",
    "    elif(inSameColumn(moveFrom, moveTo) and moveFrom[0] > moveTo[0]):\n",
    "        i = moveFrom[0]\n",
    "        while i > moveTo[0]+1:\n",
    "            i -= 1\n",
    "            if(board[i][moveTo[1]] != \"-\"):\n",
    "                return False\n",
    "            \n",
    "    elif(inSameColumn(moveFrom, moveTo) and moveFrom[0] < moveTo[0]):\n",
    "        i = moveFrom[0]\n",
    "        while i < moveTo[0]-1:\n",
    "            i += 1\n",
    "            if(board[i][moveTo[1]] != \"-\"):\n",
    "                return False\n",
    "    elif(inSameColumn(moveFrom, moveTo) and inSameRow(moveFrom, moveTo)):\n",
    "        return False\n",
    "    else:\n",
    "        return False\n",
    "    return True\n",
    "    \n",
    "\n",
    "#Tested\n",
    "def isDiagonal(moveFrom, moveTo):\n",
    "    for i in range(4):\n",
    "        if isOnBoard([moveFrom[0] + i, moveFrom[1] + i]):\n",
    "            if moveFrom[0]+i == moveTo[0] and moveFrom[1]+i == moveTo[1]:\n",
    "                return True\n",
    "            \n",
    "        if isOnBoard([moveFrom[0] - i, moveFrom[1] - i]):\n",
    "            if moveFrom[0]-i == moveTo[0] and moveFrom[1]-i == moveTo[1]:\n",
    "                return True\n",
    "        \n",
    "        if isOnBoard([moveFrom[0] + i, moveFrom[1] - i]):\n",
    "            if moveFrom[0]+i == moveTo[0] and moveFrom[1]-i == moveTo[1]:\n",
    "                return True\n",
    "        \n",
    "        if isOnBoard([moveFrom[0] - i, moveFrom[1] + i]):\n",
    "            if moveFrom[0]-i == moveTo[0] and moveFrom[1]+i == moveTo[1]:\n",
    "                return True\n",
    "    return False\n",
    "\n",
    "\n",
    "#Tested\n",
    "def doesntJumpB(board, moveFrom, moveTo):\n",
    "    diff = moveTo[0] - moveFrom[0]\n",
    "    if(diff > 0):\n",
    "        i=1\n",
    "        while i < diff:\n",
    "            if isOnBoard([moveFrom[0]+i,moveFrom[1]+i]):\n",
    "                if(board[moveFrom[0]+i][moveFrom[1]+i] != \"-\"):\n",
    "                    return False\n",
    "            i+=1\n",
    "            \n",
    "    elif(diff < 0):\n",
    "        i=-1\n",
    "        while i > diff:\n",
    "            if isOnBoard([moveFrom[0]+i,moveFrom[1]+i]):\n",
    "                if(board[moveFrom[0]+i][moveFrom[1]+i] != \"-\"):\n",
    "                    return False\n",
    "            i-=1\n",
    "            \n",
    "    else:\n",
    "        return False\n",
    "    \n",
    "    return True\n",
    "               \n",
    "    \n",
